{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "bunVersion": "1.0",
  "scripts": {
    "dev": "npm run dev",
    "build": "npm run build"
  },
  "cleanUrls": true,
  "builds": [
    "src": "src/server.ts",
    "use": "@vercel/node"
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ]
}
